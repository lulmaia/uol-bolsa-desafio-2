{
  "name": "team4",
  "version": "1.0.0",
  "main": "./src/api/server.js",
  "license": "MIT",
  "scripts": {
    "start": "node build/api/server.js",
    "build": "tsc",
    "lint": "eslint . --fix",
    "test": "jest",
    "dev": "ts-node-dev --inspect --transpile-only -r tsconfig-paths/register --ignore-watch node_modules src/api/server.ts",
    "typeorm:generate": "npx typeorm-ts-node-esm migration:create -d src/database/data-source.ts",
    "typeorm:migrate": "npx typeorm-ts-node-esm migration:run -d src/database/data-source.ts",
    "typeorm:revert": "npx typeorm-ts-node-esm migration:revert -d src/database/data-source.ts",
    "typeorm:drop": "npx typeorm-ts-node-esm schema:drop -d src/database/data-source.ts"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.9",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.3.2",
    "sqlite3": "^5.1.7",
    "ts-jest": "^29.1.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typeorm": "^0.3.20",
    "typeorm-cli": "^1.0.7",
    "typescript": "^5.5.3"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "@hapi/joi-date": "^2.0.1",
    "@types/date-fns": "^2.6.0",
    "celebrate": "^15.0.3",
    "cors": "^2.8.5",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "joi": "^17.13.3",
    "prettier": "^3.3.2",
    "reflect-metadata": "^0.2.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.20"
  }
}
